{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "users",
  "description": "The users of website",
  "required": ["email", "password", "first_name", "last_name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The database ID of the users."
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "the  first name of user"
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "the  last name of user"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "the email of user"
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "the password of user"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date of user was last updated."
    }
  }
}
